package com.nym.thread1;

public class ProcessPayment{
	
	public void withdrawal(double amount) {
		balance=balance-amount;
		System.out.println(name+" Balance is :"  + balance);
	}
	public void deposit(double amount) {
		balance=balance+amount;
	    System.out.println(name+" Balance is :"  + balance);

	}
}
